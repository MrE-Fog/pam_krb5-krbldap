#%PAM-1.0
auth       required     /lib/security/pam_nologin.so
auth       sufficient   /lib/security/pam_unix.so shadow md5 nullok likeauth
auth       required     /lib/security/pam_krb5afs.so use_first_pass
auth       required     /lib/security/pam_listfile.so item=user sense=allow file=/etc/users.local

account    required     /lib/security/pam_unix.so

password   required     /lib/security/pam_cracklib.so
password   required     /lib/security/pam_unix.so shadow md5 nullok use_authtok

session    required     /lib/security/pam_unix.so
session    optional     /lib/security/pam_krb5afs.so
